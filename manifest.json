{
  "name": "Todoist Sync",
  "description": "Sincroniza sticky notes do Miro com tarefas do Todoist usando Make.com",
  "version": "1.0.0",
  "author": "Wendley Wilson",
  "buildVersion": "1.0.0",
  "sdkVersion": "2.0",
  "scopes": [
    "boards:read",
    "boards:write"
  ],
