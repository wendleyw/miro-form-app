    "https://api.todoist.com/*",
    "https://hook.*.make.com/*"
  ],
  "icons": {
    "24": "https://cdn-icons-png.flaticon.com/24/2991/2991148.png",
    "48": "https://cdn-icons-png.flaticon.com/48/2991/2991148.png"
  },
  "tags": [
    "productivity",
    "task-management",
    "sync",
    "todoist",
    "automation"
  ]
}